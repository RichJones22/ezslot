    "swagger": "2.0",
    "paths": {
        "/api/getTradeDetails": {
            "get": {
                "tags": [
                    "Closed Trades"
                ],
                "summary": "derives trade details for a given closed trade",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "close_date",
                        "in": "query",
                        "description": "date the trade closed, ie: YYYY-MM-DD format",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "symbol",
                        "in": "query",
                        "description": "ticker symbol, ie: IBM",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                },
                "schemes": [
                    "http"
                ]
            }
        },
        "/api/closedSymbols": {
            "get": {
                "tags": [
                    "Closed Trades"
                ],
                "summary": "derive a list of closed transactions",
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "successful operation"
                    }
                },
                "schemes": [
                    "http"
                ]
            }
        }
    },
    "definitions": {}
